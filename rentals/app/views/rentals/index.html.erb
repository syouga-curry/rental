<h1>Listing rentals</h1>

<table>
  <tr>
    <th>Sdate</th>
    <th>Edate</th>
    <th>Aite tantousya</th>
    <th>Aite syamei</th>
    <th>Aite tel</th>
    <th>Tantousya</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rentals.each do |rental| %>
  <tr>
    <td><%= rental.sdate %></td>
    <td><%= rental.edate %></td>
    <td><%= rental.aite_tantousya %></td>
    <td><%= rental.aite_syamei %></td>
    <td><%= rental.aite_tel %></td>
    <td><%= rental.tantousya %></td>
    <td><%= link_to 'Show', rental %></td>
    <td><%= link_to 'Edit', edit_rental_path(rental) %></td>
    <td><%= link_to 'Destroy', rental, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rental', new_rental_path %>
